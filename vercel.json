{
  "version": 2,
  "builds": [
    { "src": "*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1.html" }
  ],
  "env": {
    "DOCKER_IMAGE": "hongfu553/highway:v1.0"
  },
  "functions": { "api/*": { "runtime": "docker" } }
}
